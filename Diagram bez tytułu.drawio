        <mxCell id="B8upwdmeaBDmH_59Xov7-13" value="Plan 1 Szczegóły:" style="rounded=0;whiteSpace=wrap;html=1;fontSize=14;" parent="1" vertex="1">
          <mxGeometry x="350" y="120" width="210" height="60" as="geometry" />
